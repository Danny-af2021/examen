<h1>Ingresar nueva noticia</h1>
<%= form_with model: @noticium do |form| %>
<div>
    <%= form.label :title %>
    <%= form.text_field :title %>
</div>

<div>
    <%= form.label :fecha %>
    <%= form.datetime_field :fecha %>
</div>

<div>
    <%= form.label :description %>
    <%= form.text_area :description %>
</div>
<%= form.submit %>
<% end %>

